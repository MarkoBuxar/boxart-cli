{
  "name": "boxart-cli",
  "description": "Videogame boxart downloader",
  "version": "0.0.1",
  "main": "dist/index.js",
  "bin": {
    "boxart-cli": "dist/index.js"
  },
  "license": "MIT",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "build": "tsc -p .",
    "global-install": "yarn build && yarn global add file:$PWD",
    "global-reinstall": "yarn global remove boxart-cli && yarn global-install"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chalk": "^2.2.0",
    "@types/clear": "^0.1.0",
    "@types/commander": "^2.12.2",
    "@types/dotenv": "^8.2.0",
    "@types/figlet": "^1.2.0",
    "@types/node": "^13.11.0",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "@types/fs-extra": "^8.1.0",
    "@types/inquirer": "^6.5.0",
    "@types/lodash": "^4.14.149"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^4.0.0",
    "clear": "^0.1.0",
    "commander": "^5.0.0",
    "dotenv": "^8.2.0",
    "figlet": "^1.3.0",
    "fs-extra": "^9.0.0",
    "inquirer": "^7.1.0",
    "lodash": "^4.17.15"
  }
